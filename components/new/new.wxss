.container {
  padding: 20rpx;
  background: #f5f5f5;
  min-height: 100vh;
}

.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
}

.page-title {
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
}

.content {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 60vh;
}

.placeholder {
  text-align: center;
}

.placeholder-image {
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 30rpx;
}

.placeholder-text {
  font-size: 32rpx;
  color: #999;
}